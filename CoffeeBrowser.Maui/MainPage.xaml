<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="CoffeeBrowser.Maui.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:coffeeBrowserLib="clr-namespace:CoffeeBrowser.Library.Components;assembly=CoffeeBrowser.Library">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent ComponentType="{x:Type coffeeBrowserLib:Routes}" Selector="#app" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
